query 1:
SELECT PATIENTS0.ROW_ID, PATIENTS0.SUBJECT_ID, PATIENTS0.GENDER, PATIENTS0.DOB, PATIENTS0.DOD, PATIENTS0.DOD_HOSP, PATIENTS0.DOD_SSN, PATIENTS0.EXPIRE_FLAG
FROM PATIENTS PATIENTS0

Q(patients_ROW_ID,patients_SUBJECT_ID,patients_GENDER,patients_DOB,patients_DOD,patients_DOD_HOSP,patients_DOD_SSN,patients_EXPIRE_FLAG) :- PATIENTS(patients_ROW_ID,patients_SUBJECT_ID,patients_GENDER,patients_DOB,patients_DOD,patients_DOD_HOSP,patients_DOD_SSN,patients_EXPIRE_FLAG)

query 2:
SELECT CHARTEVENTS0.SUBJECT_ID, PATIENTS0.GENDER
FROM CHARTEVENTS CHARTEVENTS0, PATIENTS PATIENTS0
WHERE CHARTEVENTS0.SUBJECT_ID = PATIENTS0.SUBJECT_ID
AND CHARTEVENTS0.ITEMID = 211

Q(sid,patients_GENDER) :- CHARTEVENTS(chartevents_ROW_ID,sid,chartevents_HADM_ID,chartevents_ICUSTAY_ID,211,chartevents_CHARTTIME,chartevents_STORETIME,chartevents_CGID,chartevents_VALUE,chartevents_VALUENUM,chartevents_VALUEUOM,chartevents_WARNING,chartevents_ERROR,chartevents_RESULTSTATUS,chartevents_STOPPED),PATIENTS(patients_ROW_ID,sid,patients_GENDER,patients_DOB,patients_DOD,patients_DOD_HOSP,patients_DOD_SSN,patients_EXPIRE_FLAG)

query3:
SELECT PATIENTS0.GENDER, PATIENTS0.DOB, ADMISSIONS0.INSURANCE, ADMISSIONS0.RELIGION, ADMISSIONS0.MARITAL_STATUS, ADMISSIONS0.ETHNICITY, ADMISSIONS0.DIAGNOSIS
FROM ADMISSIONS ADMISSIONS0, PATIENTS PATIENTS0
WHERE ADMISSIONS0.SUBJECT_ID = PATIENTS0.SUBJECT_ID

Q(dle,dlf,dkt,dkv,dkw,dkx,dla) :- ADMISSIONS(dkk,dkl,dkm,dkn,dko,dkp,dkq,dkr,dks,dkt,dku,dkv,dkw,dkx,dky,dkz,dla,dlb,dlc),PATIENTS(dld,dkl,dle,dlf,dlg,dlh,dli,dlj)

query 4:
SELECT CHARTEVENTS0.SUBJECT_ID, CHARTEVENTS0.HADM_ID, CHARTEVENTS0.ITEMID, CHARTEVENTS0.VALUE, CHARTEVENTS0.RESULTSTATUS
FROM CHARTEVENTS CHARTEVENTS0
WHERE CHARTEVENTS0.WARNING = 1

Q(fhn,fho,fhq,fhu,fhy) :- CHARTEVENTS(fhm,fhn,fho,fhp,fhq,fhr,fhs,fht,fhu,fhv,fhw,1,fhx,fhy,fhz)

query 5:
SELECT DIAGNOSES_ICD0.SUBJECT_ID, D_ICD_DIAGNOSES0.SHORT_TITLE
FROM D_ICD_DIAGNOSES D_ICD_DIAGNOSES0, DIAGNOSES_ICD DIAGNOSES_ICD0
WHERE D_ICD_DIAGNOSES0.ICD9_CODE = DIAGNOSES_ICD0.ICD9_CODE

Q(hkb,hjy) :- DICDDIAGNOSES(hjw,hjx,hjy,hjz),DIAGNOSESICD(hka,hkb,hkc,hkd,hjx)

query 6:
SELECT ADMISSIONS0.SUBJECT_ID, ADMISSIONS0.DIAGNOSIS, SERVICES0.CURR_SERVICE
FROM ADMISSIONS ADMISSIONS0, SERVICES SERVICES0
WHERE ADMISSIONS0.SUBJECT_ID = SERVICES0.SUBJECT_ID

Q(ild,ils,ilz) :- ADMISSIONS(ilc,ild,ile,ilf,ilg,ilh,ili,ilj,ilk,ill,ilm,iln,ilo,ilp,ilq,ilr,ils,ilt,ilu),SERVICES(ilv,ild,ilw,ilx,ily,ilz)

query 7:
SELECT ICUSTAYS0.SUBJECT_ID, ICUSTAYS0.LOS, ADMISSIONS0.DIAGNOSIS
FROM ICUSTAYS ICUSTAYS0, ADMISSIONS ADMISSIONS0
WHERE ICUSTAYS0.SUBJECT_ID = ADMISSIONS0.SUBJECT_ID

Q(lwn,lwx,lxn) :- ICUSTAYS(lwm,lwn,lwo,lwp,lwq,lwr,lws,lwt,lwu,lwv,lww,lwx),ADMISSIONS(lwy,lwn,lwz,lxa,lxb,lxc,lxd,lxe,lxf,lxg,lxh,lxi,lxj,lxk,lxl,lxm,lxn,lxo,lxp)